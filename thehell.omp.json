{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": { "cache_duration": "none" },
          "leading_diamond": "\u256d\u2500\ue0b6",
          "template": "{{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }}@{{ .HostName }} ",
          "foreground": "#D9E0EE",
          "background": "#1E1E2E",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "template": " \uf07b </>{{ path .Path .Location }} ",
          "foreground": "#1E1E2E",
          "powerline_symbol": "\ue0b0",
          "background": "#96CDFB",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "branch_icon": "\uf126 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .HEAD }} ",
          "foreground": "#1E1E2E",
          "powerline_symbol": "\ue0b0",
          "background": "#ABE9B3",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#F28FAD{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FAE3B0{{ end }}",
            "{{ if gt .Ahead 0 }}#DDB6F2{{ end }}",
            "{{ if gt .Behind 0 }}#DDB6F2{{ end }}"
          ]
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": " \ueba2 {{ .FormattedMs }} ",
          "foreground": "#1E1E2E",
          "powerline_symbol": "\ue0b0",
          "background": "#DDB6F2",
          "type": "executiontime",
          "style": "powerline"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b0",
          "template": " {{ if gt .Code 0 }}\uf00d {{ reason .Code }}{{ else }}\uf42e {{ end }}",
          "foreground": "#1E1E2E",
          "background": "#FAE3B0",
          "type": "status",
          "style": "diamond",
          "background_templates": ["{{ if gt .Code 0 }}#F38BA8{{ end }}"]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": { "cache_duration": "none" },
          "template": "\uf489 {{ .Name }} </>",
          "foreground": "#1E1E2E",
          "powerline_symbol": "\ue0b2",
          "background": "#F2CDCD",
          "type": "shell",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "properties": { "cache_duration": "none" },
          "trailing_diamond": "\ue0b4",
          "template": " \ue641 {{ .CurrentDate | date .Format }} ",
          "foreground": "#1E1E2E",
          "background": "#96CDFB",
          "type": "time",
          "style": "diamond",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": { "cache_duration": "none" },
          "template": "\u2570\u2500",
          "foreground": "#D9E0EE",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "<#F28FAD>❯</><#ABE9B3>❯</><#FAE3B0>❯</> ",
          "foreground": "#D9E0EE",
          "type": "status",
          "style": "plain",
          "foreground_templates": ["{{ if gt .Code 0 }}#F38BA8{{ end }}"]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
